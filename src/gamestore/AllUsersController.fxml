<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fillWidth="true" spacing="8.0" stylesheets="@../resources/css/style.css" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gamestore.AllUsersController">
    <padding>
        <Insets left="16.0" right="16.0" />
    </padding>
    <children>
        <Label prefWidth="610.0" text="All Users ">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
            <padding>
                <Insets right="8.0" top="8.0" />
            </padding>
        </Label>
        <Separator prefHeight="11.0" prefWidth="610.0">
            <padding>
                <Insets bottom="8.0" />
            </padding>
        </Separator>
        <TableView fx:id="allusertabel" prefHeight="300.0" prefWidth="610.0">
            <columns>

                <TableColumn fx:id="checkoutTitleCol" editable="false" prefWidth="100.0" resizable="false" sortable="false" style="-fx-alignment: center;" text="Name" />
                <TableColumn fx:id="checkoutPriceCol" editable="false" prefWidth="150.0" resizable="false" sortable="false" text="Email" />
                <TableColumn fx:id="Quantity" editable="false" prefWidth="150.0" resizable="false" sortable="false" text="Quantity" />
                <TableColumn fx:id="total" editable="false" prefWidth="150.0" resizable="false" sortable="false" text="Total" />
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>

    </children>
</VBox>
